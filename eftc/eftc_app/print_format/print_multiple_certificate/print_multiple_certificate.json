 "html": "<style>\r\n\r\n.main{\r\n    background:url({{ frappe.get_url() }}/assets/eftc/images/card_background.jpg);\r\n    background-size: 100%;\r\n    width: 700px;\r\n    height: 982px;\r\n}\r\n.main h3{\r\nfont-size: 35px;\r\npadding-top: 21rem;\r\ntext-align: center;\r\n    font-family: 'Inter';\r\n    font-weight: 500;\r\n}\r\n.para{\r\n    font-size: 16px;\r\n    font-weight: 600;\r\ntext-align: center;\r\n}\r\n.para1{\r\n    text-align: center;\r\n    color: red;\r\n    font-weight: 600;\r\n    font-size: 20px;\r\n    padding-left: 16rem;\r\n}\r\n.main h2{\r\npadding-top: 4.5rem;\r\n    text-align: center;\r\n    font-size: 35px;\r\n    font-family: 'Inter';\r\n    font-weight: 500;\r\n}\r\n.flex{\r\nfont-size: 20px;\r\ndisplay: flex;\r\njustify-content: space-evenly;\r\n}\r\n</style>\r\n<body>\r\n    {% for attendee in doc.attendees %}\r\n     {% set certificate = frappe.get_doc('Certificate', {'training_schedule': doc.name, 'attendee_name': attendee.attendee_name}) %}\r\n    <div class=\"main\">\r\n        <h6>&nbsp;</h6>\r\n        <h3>{{ attendee.attendee_name }}</h3>\r\n        <p class=\"para\" style=\"margin-top:1rem;\">IQAMA/ID NO {{ attendee.iqamaid_no }}</p>\r\n        <h2>WORK AT HEIGHT</h2>\r\n        <div class=\"row flex\" style=\"  margin-left:8rem;  font-size: 14px;\">\r\n            <div class=\"col-md-6\">ISSUE DATE: {{ attendee.issue_date }}</div>\r\n            <div class=\"col-md-6\">EXPIRY DATE: {{ certificate.expiry }}</div>\r\n        </div>\r\n        <div style=\"display: flex; justify-content: space-evenly; align-items: center; margin-top: 7rem;\">\r\n    <p class=\"para1\">\r\n        {% if certificate %}\r\n            {{ certificate.name }}\r\n        {% else %}\r\n            {{ doc.name }}\r\n        {% endif %}\r\n    </p>\r\n    {% if certificate and certificate.qr_image %}\r\n        <img style=\"height: 60px; width: 60px; float: right; margin-left:8rem; margin-top: -1rem;\" src=\"{{certificate.qr_image}}\">\r\n    {% endif %}\r\n</div>\r\n        </div>\r\n    <br>\r\n    <div style=\"page-break-before: always;\"></div>\r\n    {% endfor %}\r\n</body>",
