 "css": "\ntable, th, td {\n  border: 2px solid black;\n}\n\n.Divbox{\n   border: 1px solid grey;\nheight: auto;\nwidth: 300px;\npadding: 20px;\n}\n\n.barcode-box{\nheight: auto;\nwidth: 300px\n}\n\n.barcode {\ndisplay: flex;\njustify-content: space-between;\n}\n\n.bar-img{\n    height:50px !important;\n    width:200px;\n}\n\n\n\n.divone, .divtwo{\n    background-color:#adabab;\n}\n\n*{\n    font-style:oblique;\n    font-family:cursive !important;\n}",
 "custom_format": 1,
 "default_print_language": "en",
 "disabled": 0,
 "doc_type": "Quotation",
 "docstatus": 0,
 "doctype": "Print Format",
 "font_size": 14,
 "html": "\n    <!--Header Section-->\n    {% macro header() %}\n     <div class=\"col-xs-12\">\n            <img src=\"/assets/eftc/images/header.png\" >\n    </div>\n    {% endmacro %}\n   \n   {% macro page_end() %}\n         <div class=\"col-xs-12\"  >\n            <img style = \"margin-top:100px;\" src=\"/assets/eftc/images/footer.png\" >\n    </div>\n   <div style = \"page-break-after: always;\"></div>\n   {% endmacro %}\n   \n   {{ header ()}}\n    <div class=\"col-xs-12\">\n             <p class=\"heading-right\"><b style=\"font-size: 20px;padding-left: 200px;\">SALES QUOTATION</b></p>\n     </div>\n    \n    <!--Body Section-->\n    <div class = \"row\">\n        <div class = \"col-xs-4\" style = \"margin-left: 32px;\">\n            <p><b>To,</b></p>\n            <p>{{doc.address_display}}</p>\n        </div>\n        <div class = \"col-xs-7\" style = \"padding-left:64px;\">\n            <div class = \"col-xs-6\" style = \"text-align:left;\">Date:</div>\n            <div class = \"col-xs-6\" style = \"text-align:center;\">{{doc.transaction_date}}</div>\n            <div class = \"col-xs-6\" style = \"text-align:left;\">Quotation Ref:</div>\n            <div class = \"col-xs-6\" style = \"text-align:center;\">{{doc.name}}</div>\n        </div>\n    </div>\n\n<div class =\"row\">\n                <div class = \"col-xs-12\" style=\"margin-left: 20px;margin-right: 40px;\">\n                    <table class=\"table\" style = \"width:95%;\">\n                        <tr>\n                            <th ><b>Sales REP</b></th>\n                            <th ><b>Mobile No</b></th>\n                            <th ><b>Payment Terms</b></th>\n                            <th ><b>Qtn. Validation</b></th>\n                        </tr>\n                        <tr> \n                            <td  >{{doc.custom_sales_representative}} </td>\n                            <td >{{doc.sales_representative_mobile_number}} </td>\n                            <td> 100% Advance\n                            </td>\n                            <td  >{{doc.qr_code_validity_in_days}} </td>\n                        </tr>\n                    </table>\n                </div>\n    </div>\n       \n            \n    <div class = \"col-xs-12\">\n        <p style = \"margin:10px 10px 3px 0px;\">Dear Sir ,<br>We Are Greatly privileged to be given this chance to quote for your company . We have evaluated your requirements in details and we came up with a propasal that is appropriate for your need.</p>\n    </div>\n    \n    \n    \n    \n    <div class =\"row\">\n    <div class = \"col-xs-12\" style=\"margin-left: 20px;margin-right: 40px;\">\n                    <table class=\"table\" style = \"width:95%;\">\n                        <tr>\n                            <th style = \"width:10%;\"><b>SL.NO</b></th>\n                            <th style = \"width:10%;\"><b>COURSE</b></th>\n                            <th style = \"width:30%;\"><b>AWARDING BODY</b></th>\n                            <th style = \"width:20%;\"><b>UNIT PRICE</b></th>\n                            <th style = \"width:10%;\"><b>QUANTITY</b></th>\n                            <th style = \"width:20%;\"><b>TOTAL (SR)</b></th>\n                        </tr>\n        {% for item in doc.items %}\n        <tr>\n              <td  style = \"text-align:right;\">{{item.idx}} </td>\n              <td  >{{item.item_name}} </td>\n              <td  >{{item.item_name}} </td>\n              <td  >{{item.rate}} </td>\n              <td  >{{item.qty}} </td>\n              <td  >{{item.base_amount}} </td>\n       \n        {% endfor %}\n         </tr>\n        </table>\n        </div>\n        </div>\n  \n    \n   \n    \n    \n<div class =\"row\" style=\"\">\n    <div class = \"col-xs-5\">\n        <img style = \"height: 90px;width:90px;margin-left:44px\"src= {{doc.qr_image}}>\n    </div>\n                <div class = \"col-xs-7\" style=\"width: 66%;\n    padding: 0px;\n    margin-top: -109px;\n    float: right;    margin-right: 4px;\n\">\n                    <table style = \"width:95%;font-size: small;\">\n                        <tr> \n                            <td><b>SUB TOTAL </b></td>\n                            <td >{{ doc.get_formatted(\"total\") }}</td>\n                        </tr>\n                        <tr> \n                            <td><b> VAT TOTAL </b></td>\n                            <td > {{ doc.get_formatted(\"total_taxes_and_charges\") }}</td>\n                        </tr>\n                        <tr> \n                            <td><b>GRAND TOTAL </b></td>\n                            <td > {{ doc.get_formatted(\"base_grand_total\") }}</td>\n                        </tr>\n                    </table>\n                </div>\n    </div>\n    \n    {{page_end()}}\n    {% if doc.tc_name %}\n    {{header()}}\n   <div class = \"row\" style = \"margin-left:20px;margin-top:30px;width:95%;\">\n       <div style = \"background-color:grey;padding: 5px;\"><p><b>Terms and Conditons:</b></div>\n    <br>{{doc.terms}}</p>\n   \n     <div class=\"col-xs-12\"  >\n            <img style = \"margin-top:100px;\" src=\"/assets/eftc/images/footer.png\" >\n    </div>\n    \n    {% endif %}\n",
